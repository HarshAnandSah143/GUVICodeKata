#include <stdio.h>
#include <string.h>
int main() {
    int n, m, r, c;
    scanf("%d %d", &n, &m);
    char grid[100][101];
    for (int i = 0; i < n; i++) {
        scanf("%s", grid[i]);
    }
    scanf("%d %d", &r, &c);
        int bottom_r = n - 1;
    int bottom_c = m - 1;
    if (c + 1 < m && grid[r][c + 1] == '.' && 
        ((r + c + 1) < (bottom_r + bottom_c))) {
        printf("RIGHT\n");
        return 0;
    }
        if (r + 1 < n && grid[r + 1][c] == '.' && 
        ((r + 1 + c) < (bottom_r + bottom_c))) {
        printf("DOWN\n");
        return 0;
    }
        if (c - 1 >= 0 && grid[r][c - 1] == '.') {
        printf("LEFT\n");
        return 0;
    }
        printf("UP\n");
    return 0;
}
