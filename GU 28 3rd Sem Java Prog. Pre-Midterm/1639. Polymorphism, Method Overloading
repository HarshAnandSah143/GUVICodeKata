 import java.util.Scanner;
 import java.math.BigInteger;
 import java.math.BigDecimal;
 class Calculator {
    public static String calculateInt(String a, String b) {
        BigInteger x = new BigInteger(a.trim());
        BigInteger y = new BigInteger(b.trim());
        return x.add(y).toString();
    }
    public static String calculateFloat(String a, String b) {
        BigDecimal x = new BigDecimal(a.trim());
        BigDecimal y = new BigDecimal(b.trim());
        BigDecimal sum = x.add(y);
        return sum.stripTrailingZeros().toPlainString();
    }
    public static String calculateString(String a, String b) {
        return a + b;
    }
 }
 public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String dataType = "";
        while (sc.hasNextLine()) {
            dataType = sc.nextLine().trim();
            if (!dataType.isEmpty()) break;
        }
        dataType = dataType.toLowerCase();
        String v1 = "";
        String v2 = "";
        if (sc.hasNextLine()) v1 = sc.nextLine();
        if (sc.hasNextLine()) v2 = sc.nextLine();
        switch (dataType) {
            case "int":
            case "integer":
                System.out.println(Calculator.calculateInt(v1, v2));
                break;
            case "float":
            case "double":
                System.out.println(Calculator.calculateFloat(v1, v2));
                break;
            case "string":
                System.out.println(Calculator.calculateString(v1, v2));
                break;
                 default:
 System.out.println("Invalid data type");
        }
 sc.close();
    }
 }