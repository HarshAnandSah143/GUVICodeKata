import java.util.*;
interface BookOperations {
    void addBook(String book);
}
interface UserOperations {
    void borrowBook(String book);
}
class Library implements BookOperations, UserOperations {
    List<String> books = new ArrayList<>();
    @Override
    public void addBook(String book) {
        books.add(book);
        System.out.println("Book added: " + book);
    }
    @Override
    public void borrowBook(String book) {
        if (books.contains(book)) {
            books.remove(book);
            System.out.println("Book borrowed: " + book);
        } else {
            System.out.println("Book not available");
        }
    }
}
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String bookToAdd = sc.nextLine();
        String borrowCommand = sc.nextLine();
        Library library = new Library();
        library.addBook(bookToAdd);
        if (borrowCommand.toLowerCase().startsWith("borrow ")) {
            String bookToBorrow = borrowCommand.substring(7).trim();
            library.borrowBook(bookToBorrow);
        }
    }
}