import java.util.*;
class LibraryItem {
    int lateDays;
    LibraryItem(int lateDays) {
        this.lateDays = lateDays;
    }
    public int calculateLateFee() {
        return 0;
    }
}
class Book extends LibraryItem {
    Book(int lateDays) {
        super(lateDays);
    }
    @Override
    public int calculateLateFee() {
        return lateDays * 5;
    }
}
class Magazine extends LibraryItem {
    Magazine(int lateDays) {
        super(lateDays);
    }
    @Override
    public int calculateLateFee() {
        return lateDays * 2;
    }
}
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String type = sc.nextLine().toLowerCase();
        int days = sc.nextInt();
        LibraryItem item;
        if (type.equals("book")) {
            item = new Book(days);
        } else {
            item = new Magazine(days);
        }
        System.out.println(item.calculateLateFee());
    }
}