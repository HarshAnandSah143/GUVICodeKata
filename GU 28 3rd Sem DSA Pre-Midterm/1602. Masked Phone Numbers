#include <stdio.h>
#include <string.h>
#include <ctype.h>
int main() {
    char s[10005];
    fgets(s, sizeof(s), stdin);
    int len = strlen(s), i = 0;
    while (i < len) {
        int digits = 0;
        int pos[20]; 
        int j = i;
        while (j < len && s[j] != ' ' && s[j] != '\n') {
            if (isdigit(s[j]))
            pos[digits++] = j;
            j++;
        }
        int d;
        for (d = 0; d < digits - 4; d++)
        s[pos[d]] = '*';
        for (int k = i; k < j; k++)
        putchar(s[k]);
        if (s[j] == ' ')
        putchar(' ');
        i = j + 1;
    }
    return 0;
}