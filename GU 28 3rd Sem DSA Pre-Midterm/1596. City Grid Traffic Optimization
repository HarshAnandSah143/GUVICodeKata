#include <stdio.h>
#define MAX 100
int main() {
    int M, N, i, j;
    scanf("%d %d", &M, &N);
    int grid[MAX][MAX], min_time[MAX][MAX];
    for (i = 0; i < M; i++)
    for (j = 0; j < N; j++)
    scanf("%d", &grid[i][j]);
    min_time[0][0] = grid[0][0];
    for (j = 1; j < N; j++)
    min_time[0][j] = min_time[0][j-1] + grid[0][j];
    for (i = 1; i < M; i++)
    min_time[i][0] = min_time[i-1][0] + grid[i][0];
    for (i = 1; i < M; i++)
    for (j = 1; j < N; j++)
    min_time[i][j] = grid[i][j] + (min_time[i-1][j] < min_time[i][j-1] ? min_time[i-1][j] : 
    min_time[i][j-1]);
    printf("%d\n", min_time[M-1][N-1]);
    return 0;
}