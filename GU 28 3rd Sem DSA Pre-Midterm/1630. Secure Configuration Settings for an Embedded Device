#include <stdio.h>
int main() {
    int n;
    scanf("%d", &n);
    if (n < 1 || n > 5) {
        return 1; 
    }
    float temp[n];
    for (int i = 0; i < n; i++) {
        scanf("%f", &temp[i]);
    }
    const float * const config = temp;
    printf("Configuration Values:\n");
    for (int i = 0; i < n; i++) {
        printf("%.5f\n", config[i]);
    }
    return 0;
}