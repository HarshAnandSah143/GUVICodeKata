import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String input = sc.nextLine();
        String[] parts = input.split(",");
        String a = parts[0].split("=")[1];
        int b = Integer.parseInt(parts[1].split("=")[1]);
        String result = divideString(a, b);
        System.out.println(result);
    }
    public static String divideString(String number, int divisor) {
        StringBuilder result = new StringBuilder();
        int remainder = 0;
        for (int i = 0; i < number.length(); i++) {
            int digit = number.charAt(i) - '0';
            int current = remainder * 10 + digit;
            result.append(current / divisor);
            remainder = current % divisor;
        }
        String ans = result.toString().replaceFirst("^0+(?!$)", "");
        return ans.isEmpty() ? "0" : ans;
    }
}